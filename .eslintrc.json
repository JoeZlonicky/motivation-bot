{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "standard",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": ["error", "single"],
        "indent": ["error", 4],
        "semi": ["error", "always"],
        "no-multi-spaces": ["error"]
    }
}
